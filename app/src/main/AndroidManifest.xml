<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.deafears">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PlayingActivity"
            android:label="@string/playing"/>
        <activity
            android:name=".PlaylistsActivity"
            android:label="@string/playlist"/>
        <activity
            android:name=".ExploreActivity"
            android:label="@string/explore"/>
        <activity
            android:name=".LibraryActivity"
            android:label="@string/library"/>
        <activity
            android:name=".AlbumActivity"
            android:label="@string/this_album"/>
        <activity
            android:name=".SearchResultsActivity"
            android:label="@string/search_music"/>
        <activity
            android:name=".TrendingActivity"
            android:label="@string/trending"/>
        <activity
            android:name=".FavouritesActivity"
            android:label="@string/favourites">
        </activity>
    </application>

</manifest>